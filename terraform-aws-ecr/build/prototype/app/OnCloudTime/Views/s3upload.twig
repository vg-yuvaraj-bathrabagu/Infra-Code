<div class="card">
    <div class="card-header">
        <h4 class="card-title">S3 Files</h4>
        <a class="refresh pull-right" href="javascript:void(0);"><i class="fa fa-refresh fa-2x fa-fw"></i></a>
        <br />
    </div>
        <div class="card-content">
		<div class="table-responsive">
            <table class="table header-fixed">
                <thead>
                    <tr>
                    	<th>Action</th>
                        <th>File</th>
                        <!-- <th class="text-center">Column Names</th>
                        <th class="text-center">Create Table</th> -->
                    </tr>
                </thead>
                <tbody>
                	{% set increment = 0 %}

					{% for report in customReports %}
					     {% set increment = increment + 1 %}
                    <tr>
                        <td>
                        	<a href="#">
                        		<i class="fa fa-eye" aria-hidden="true"></i>
                        	</a>
                        	<a href="javascript:void(0);" class="del" title = "Delete File" data-file="{{report.Key}}">
                        		<i class="fa fa-trash-o" aria-hidden="true"></i>
                        	</a>
                        	<a href="javascript:void(0)" class="orc" data-file="{{report.Key}}" title="Conver to ORC">
                        		<i class="fa fa-exchange" aria-hidden="true"></i>
                        	</a>
                        	<a href="javascript:void(0);" class="par" data-file="{{report.Key}}" title="Conver to Partquet">
                        		<i class="ti-exchange-vertical" aria-hidden="true"></i>
                        	</a>
                        	<a class="copy" title="Copy to Athena Folder" data-toggle="modal" href="loadTree?file={{report.Key|url_encode}}" data-target="#myModal" data-cache="false" data-file="{{report.Key}}">
                        		<i class="fa fa-files-o" aria-hidden="true"></i>
                        	</a>
                        </td>
                        <td>
                            {{ report.Key }}
                        </td>
                        <!-- <td class="td-product">
                            <div class="form-group">
                                <textarea class="form-control" placeholder="Provide coma sperated column names with datatype. (eg: surname STRING,firstname STRING)" rows="5"></textarea>
                            </div>
                        </td>
                        <td class="text-center">
                        	<a data-file="{{report.Key}}" href="javascript:void(0);" class="add-table">
                            <i class="ti-plus icon-danger"></i>
                            </a>
                        </td> -->
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
		</div>
    </div><!--  end card  -->

<!-- Modal -->


